{
    "basePath": "https://devapi.cloudwalkers.be",
    "swaggerVersion": "1.2",
    "apiVersion": "1",
    "resourcePath": "/reports",
    "apis": [
        {
            "path": "/reports",
            "description": "Operation on reports",
            "operations": [
                {
                    "method": "GET",
                    "summary": "Get a list of reports",
                    "nickname": "getFromIds",
                    "parameters": [
                        {
                            "paramType": "query",
                            "name": "ids",
                            "type": "string",
                            "required": true,
                            "description": "List of ids, comma seperated"
                        }
                    ]
                }
            ]
        },
        {
            "path": "/reports/{id}",
            "description": "Operation on reports",
            "operations": [
                {
                    "method": "GET",
                    "summary": "Get a specific report",
                    "nickname": "getFromId",
                    "parameters": [
                        {
                            "paramType": "path",
                            "name": "id",
                            "type": "integer",
                            "required": true,
                            "description": "Report id"
                        }
                    ]
                }
            ]
        }
    ]
}