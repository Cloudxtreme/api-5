{
    "basePath": "https://devapi.cloudwalkers.be",
    "swaggerVersion": "1.2",
    "apiVersion": "1",
    "resourcePath": "/notifications",
    "apis": [
        {
            "path": "/notifications",
            "description": "Operation on notifications",
            "operations": [
                {
                    "method": "GET",
                    "summary": "Get a set of notifications. ids must be provided.",
                    "nickname": "getData",
                    "parameters": [
                        {
                            "paramType": "query",
                            "name": "ids",
                            "type": "string",
                            "required": true,
                            "description": "Comma seperate list of message ids"
                        }
                    ]
                }
            ]
        },
        {
            "path": "/notifications/{id}",
            "description": "Operation on notifications",
            "operations": [
                {
                    "method": "GET",
                    "summary": "Get a specific notification by id",
                    "nickname": "getFromId",
                    "parameters": [
                        {
                            "paramType": "path",
                            "name": "id",
                            "type": "integer",
                            "required": true,
                            "description": "Notification id"
                        }
                    ]
                }
            ]
        }
    ]
}